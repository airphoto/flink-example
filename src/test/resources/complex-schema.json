{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "description": "A representation of a person, company, organization, or place",
  "type": "object",
  "required": [
    "familyName",
    "givenName"
  ],
  "properties": {
    "fn": {
      "description": "Formatted Name",
      "type": "string"
    },
    "familyName": {
      "type": "string"
    },
    "additionalName": {
      "type": "boolean"
    },
    "tuples": {
      "type": "array",
      "items": [
        {
          "type": "number"
        },
        {
          "type": "string"
        },
        {
          "type": "string",
          "enum": [
            "Street",
            "Avenue",
            "Boulevard"
          ]
        },
        {
          "type": "string",
          "enum": [
            "NW",
            "NE",
            "SW",
            "SE"
          ]
        }
      ],
      "additionalItems": false
    },
    "honorificPrefix": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "email": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "email"
        }
      }
    },
    tel: {
      "type": "object",
      "properties": {
        "type": {
          "type": "integer"
        },
        "value": {
          "type": "string",
          "format": "phone"
        }
      }
    },
    "sound": {
      "type": "null"
    },
    "org": {
      "type": "object",
      "properties": {
        "organizationUnit": {
          "type": "object",
          "properties": {}
        }
      }
    }
  }
}
